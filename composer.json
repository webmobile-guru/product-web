{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "aws/aws-sdk-php-laravel": "^3.3",
        "buzz/laravel-google-captcha": "^2.1",
        "cboden/ratchet": "^0.4.1",
        "fideloper/proxy": "~3.3",
        "gregwar/captcha": "^1.1",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/passport": "4.0.3",
        "laravel/tinker": "~1.0",
        "mramitict/coinpayment": "^0.0.3",
        "phanan/remote": "^2.0",
        "pragmarx/google2fa": "^2.0",
        "twilio/sdk": "^5.34"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "ratchet/pawl": "^0.3.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
